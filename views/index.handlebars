    <div id="products" class="product-container">
        <div class="row">
          <div class="col-md-6 col-md-offset-3">
            <h2>Members Page</h2>
            <div class="col s12 m12">
                <div class="card horizontal">
                  <div class="card-stacked">
                    <div class="card-content">
                     <ul>  
                        {{#each product}}
                        <li>{{this.product_name}}, {{this.quantity}}, {{this.min_length}} {{this.min_lengthUnits}}, {{this.rate}}, {{this.Company.name}}, <a href="{{this.contract}}">View Contract</a></li>
                        <li><p>{{this.product_description}} - {{this.category}}</p></li>
                        {{/each}}
                     </ul>
                    </div>
                    <div class="card-action">
                      <a href="#">Request info</a>
                    </div>
                  </div>
                </div>
              </div>
          </div>
        </div>
    </div>

